{
	"printWidth": 120,
	"semi": true,
	"tabWidth": 2,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"useTabs": false,
	"bracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"overrides": [{ "files": "*.scss", "options": { "parser": "scss" } }]
}
